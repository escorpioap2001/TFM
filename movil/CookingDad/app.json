{
  "expo": {
    "name": "CookingDad",
    "slug": "CookingDad",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5353ec"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.palpu.CookingDad"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#5353ec"
      },
      "package": "com.palpu.CookingDad",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "047641a3-2339-48ff-8e83-b02713b00c9c"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you change your avatar."
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/047641a3-2339-48ff-8e83-b02713b00c9c"
    }
  },
  "dependencies": {
      "@react-navigation/bottom-tabs": "^6.2.0",
      "@react-navigation/native": "^6.0.8",
      "@react-navigation/native-stack": "^6.5.0",
      "expo": "~44.0.0",
      "expo-image-picker": "^12.0.2",
      "expo-location": "^14.0.2",
      "expo-status-bar": "~1.2.0",
      "expo-updates": "~0.11.6",
      "firebase": "9.6.7",
      "formik": "^2.2.9",
      "intl": "^1.2.5",
      "luxon": "^2.3.1",
      "react": "17.0.1",
      "react-dom": "17.0.1",
      "react-native": "0.64.3",
      "react-native-elements": "^3.4.2",
      "react-native-get-random-values": "^1.7.2",
      "react-native-keyboard-aware-scroll-view": "^0.9.5",
      "react-native-maps": "0.29.4",
      "react-native-open-maps": "^0.4.0",
      "react-native-safe-area-context": "3.3.2",
      "react-native-screens": "~3.10.1",
      "react-native-snap-carousel": "^3.9.1",
      "react-native-toast-message": "^2.1.1",
      "react-native-vector-icons": "^9.1.0",
      "react-native-web": "0.17.1",
      "uuid": "^8.3.2",
      "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9"
  },
  "private": true
}
